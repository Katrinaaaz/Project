{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-plusplus": "off",
        "eol-last": "off",
        "max-len": [
            "error",
            {
                "code": 80,
                "ignoerUrls": true
            }
        ],
        "no-alert": "off",
        "no-param-reassign": "off",
        "no-tabs": "off",
        "prefer-destructuring": "off"
    }
}